// Step 1: Complete this if statement so that is guarantees both fields have a value
        if(!$("#firstName").val() || !$("#phoneNumber").val()) {
            // Step 2: Show the user a message that their form is incomplete
            return false;
        }
        
// Step 3:
    var processForm = function(form, url, data) {
        // Get the status from the validate function
        var isValid = validate(form);
        if(isValid) {
            $.ajax({
              type: "POST",
              url: url,
              data: data,
              success: function(res) {
                var result = $.parseJSON(res);
                showModal(result.status, result.message);
              },
              dataType: "text"
            });
        } else {
            showModal('Error', 'You must fill out all fields to submit the form');
        }    
    };
    
// Step 2
showModal('Error', 'You must fill out all fields to submit the form');